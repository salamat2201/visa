msgstr "Тилди тандаңыз:"

msgid "Все права защищены."
msgstr "Бардык укуктар корголгон."

# Навигация
msgid "О нас"
msgstr "Биз жөнүндө"

msgid "Менеджеры"
msgstr "Менеджерлер"

msgid "Направления виз"
msgstr "Виза багыттары"

msgid "Отзывы"
msgstr "Пикирлер"

